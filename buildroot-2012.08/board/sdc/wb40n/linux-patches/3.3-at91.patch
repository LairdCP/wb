diff -urN linux-3.3/arch/arm/mach-at91/ics1523.c linux-3.3-at91/arch/arm/mach-at91/ics1523.c
--- linux-3.3/arch/arm/mach-at91/ics1523.c	1969-12-31 19:00:00.000000000 -0500
+++ linux-3.3-at91/arch/arm/mach-at91/ics1523.c	2012-05-10 19:47:46.339995904 -0400
@@ -0,0 +1,208 @@
+/*
+ * arch/arm/mach-at91rm9200/ics1523.c
+ *
