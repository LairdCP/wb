BR2_arm=y
BR2_arm926t=y
BR2_WGET="wget --passive-ftp -nd"
BR2_DL_DIR="$(TOPDIR)/../archive"
BR2_SOURCEFORGE_MIRROR="easynews"
BR2_JLEVEL=16
BR2_STRIP_sstrip=y
BR2_TOOLCHAIN_EXTERNAL=y
BR2_TOOLCHAIN_EXTERNAL_CUSTOM=y
BR2_TOOLCHAIN_EXTERNAL_PATH=""
BR2_TOOLCHAIN_EXTERNAL_CUSTOM_PREFIX="arm-sdc-linux-gnueabi"
BR2_TOOLCHAIN_EXTERNAL_CUSTOM_GLIBC=y
BR2_TOOLCHAIN_EXTERNAL_CXX=y
BR2_ENABLE_LOCALE_PURGE=y
BR2_ENABLE_LOCALE_WHITELIST="en_US"
BR2_TARGET_GENERIC_HOSTNAME="summit"
BR2_TARGET_GENERIC_ISSUE="Summit Data Communications"
BR2_ROOTFS_DEVICE_CREATION_DYNAMIC_UDEV=y
BR2_ROOTFS_DEVICE_TABLE="board/sdc/device_table.txt"
BR2_ROOTFS_POST_BUILD_SCRIPT="board/sdc/customers/carefusion/post_build.sh"
BR2_PACKAGE_BUSYBOX_CONFIG="$(TOPDIR)/board/sdc/customers/carefusion/configs/busybox-1.19.x.config"
BR2_PACKAGE_BUSYBOX_SHOW_OTHERS=y
BR2_PACKAGE_MTD=y
# BR2_PACKAGE_MTD_FLASHCP is not set
# BR2_PACKAGE_MTD_FLASH_ERASEALL is not set
# BR2_PACKAGE_MTD_FLASH_INFO is not set
# BR2_PACKAGE_MTD_FLASH_LOCK is not set
# BR2_PACKAGE_MTD_FLASH_UNLOCK is not set
# BR2_PACKAGE_MTD_MTD_DEBUG is not set
# BR2_PACKAGE_MTD_NANDTEST is not set
BR2_PACKAGE_SETSERIAL=y
BR2_PACKAGE_LIBGCRYPT=y
BR2_PACKAGE_OPENSSL=y
BR2_PACKAGE_OPENSSL_BIN=y
BR2_PACKAGE_LIBUSB=y
BR2_PACKAGE_PROTOBUF=y
BR2_PACKAGE_BRIDGE_UTILS=y
BR2_PACKAGE_EBTABLES=y
BR2_PACKAGE_IPTABLES=y
BR2_PACKAGE_IW=y
BR2_PACKAGE_PPPD=y
BR2_PACKAGE_PROFTPD=y
BR2_PACKAGE_LOGROTATE=y
BR2_PACKAGE_NCM=y
BR2_PACKAGE_UTIL_LINUX=y
BR2_TARGET_ROOTFS_UBIFS=y
BR2_TARGET_ROOTFS_UBIFS_LEBSIZE=0x3e00
BR2_TARGET_ROOTFS_UBIFS_MINIOSIZE=0x200
BR2_TARGET_ROOTFS_UBIFS_MAXLEBCNT=1888
BR2_TARGET_ROOTFS_UBI=y
BR2_TARGET_ROOTFS_UBI_PEBSIZE=0x4000
BR2_TARGET_ROOTFS_UBI_SUBSIZE=256
BR2_TARGET_ROOTFS_TAR_BZIP2=y
BR2_TARGET_UBOOT=y
BR2_TARGET_UBOOT_BOARDNAME="wb40n"
BR2_TARGET_UBOOT_CUSTOM_GIT=y
BR2_TARGET_UBOOT_CUSTOM_PATCH_DIR="$(TOPDIR)/board/sdc/wb40n/patches"
BR2_TARGET_UBOOT_CUSTOM_GIT_REPO_URL="git://git.denx.de/u-boot.git"
BR2_TARGET_UBOOT_CUSTOM_GIT_VERSION="ebbf0d20aa85f623c49b7ed3349ebfea450c152d"
BR2_TARGET_AT91BOOTSTRAP3=y
BR2_TARGET_AT91BOOTSTRAP3_CUSTOM_PATCH_DIR="$(TOPDIR)/board/sdc/wb40n/patches"
BR2_TARGET_AT91BOOTSTRAP3_DEFCONFIG="wb40n"
BR2_LINUX_KERNEL=y
BR2_LINUX_KERNEL_CUSTOM_VERSION=y
BR2_LINUX_KERNEL_CUSTOM_VERSION_VALUE="3.2"
BR2_LINUX_KERNEL_PATCH="$(TOPDIR)/board/sdc/wb40n/patches"
BR2_LINUX_KERNEL_DEFCONFIG="wb40n"
BR2_SDC_PLATFORM_WB40N=y
BR2_PACKAGE_LIBEDIT=y
BR2_PACKAGE_SDCSDK=y
BR2_PACKAGE_SDCCLI=y
BR2_PACKAGE_SDCSUPP=y
BR2_PACKAGE_DHD=y
