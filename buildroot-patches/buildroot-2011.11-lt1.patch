diff -Naur old/package/netsnmp/mibs/LAIRD-CLI-MIB.txt new/package/netsnmp/mibs/LAIRD-CLI-MIB.txt
--- old/package/netsnmp/mibs/LAIRD-CLI-MIB.txt	1970-01-01 01:00:00.000000000 +0100
+++ new/package/netsnmp/mibs/LAIRD-CLI-MIB.txt	2012-09-13 20:57:18.375000000 +0100
@@ -0,0 +1,109 @@
+LAIRD-CLI-MIB DEFINITIONS ::= BEGIN
+
+--
+-- Laird (Summit) Command Line Interface MIB
+--
+
+IMPORTS
+    MODULE-IDENTITY, OBJECT-TYPE, Integer32,
+    NOTIFICATION-TYPE                       FROM SNMPv2-SMI
+    SnmpAdminString                         FROM SNMP-FRAMEWORK-MIB
+    laird                                   FROM LAIRD-MIB
+    MacAddress, RowStatus, StorageType,
+    TEXTUAL-CONVENTION                      FROM SNMPv2-TC
+    InetAddressType, InetAddress            FROM INET-ADDRESS-MIB
+;
+
+lairdSnmpCLIMIB MODULE-IDENTITY
+    LAST-UPDATED "201208160000Z"
+    ORGANIZATION "www.lairdtech.com"
+    CONTACT-INFO    
+	 "postal:   Tim Carney
+                    11160 Thompson Ave
+                    Lenexa, KS 66219 USA
+
+          email:    wireless-support@lairdtech.com"
+    DESCRIPTION
+	"Laird CLI MIB"
+    REVISION     "201208160000Z"
+    DESCRIPTION
+	"First draft"
+    ::= { laird 3 }
+
+--
+-- top level structure
+--
+lairdSnmpCliStatus        OBJECT IDENTIFIER ::= { lairdSnmpCLIMIB 1 }
+lairdSnmpCliProfileTable  OBJECT IDENTIFIER ::= { lairdSnmpCLIMIB 2 }
+
+
+--
+-- Laird CLI Status
+--
+
+statusConfigName    OBJECT-TYPE
+    SYNTAX          String
+    MAX-ACCESS      read-write
+    STATUS          current
+    DESCRIPTION
+	"User-assigned configuration name"
+    DEFVAL { "Default" }
+    ::= { lairdSnmpCliStatus 1 }
+
+statusChannel       OBJECT-TYPE
+    SYNTAX          Integer32
+    MAX-ACCESS      read-write
+    STATUS          current
+    DESCRIPTION
+	"Channel number"
+    ::= { lairdSnmpCliStatus 2 }
+    
+statusRSSI          OBJECT-TYPE
+    SYNTAX          Integer32 (-120 .. 40) --tjc - determine the RSSI range of the WB40n
+    MAX-ACCESS      read-only
+    STATUS          current
+    DESCRIPTION
+	"Receive signal strength indicator"
+    ::= { lairdSnmpCliStatus 3 }
+    
+statusDeviceName    OBJECT-TYPE
+    SYNTAX          String
+    MAX-ACCESS      read-write
+    STATUS          current
+    DESCRIPTION
+	"User-assigned device name"
+    ::= { lairdSnmpCliStatus 4 }
+    
+statusMAC           OBJECT-TYPE
+    SYNTAX          MacAddress
+    MAX-ACCESS      read-only
+    STATUS          current
+    DESCRIPTION
+	"Hardware Media Access Control (MAC) address"
+    ::= { lairdSnmpCliStatus 5 }
+
+statusIP            OBJECT-TYPE
+    SYNTAX          InetAddress
+    MAX-ACCESS      read-write
+    STATUS          current
+    DESCRIPTION
+	"Internet Protocol (IP) address"
+    ::= { lairdSnmpCliStatus 6 }
+    
+statusAPName        OBJECT-TYPE
+    SYNTAX          String
+    MAX-ACCESS      read-only
+    STATUS          current
+    DESCRIPTION
+	"Name of the current Access Point (AP)"
+   ::= { lairdSnmpCliStatus 7 }    
+    
+statusAPMAC         OBJECT-TYPE
+    SYNTAX          MacAddress
+    MAX-ACCESS      read-only
+    STATUS          current
+    DESCRIPTION
+	"Hardware Media Access Control (MAC) address for the current Access Point (AP)"
+    ::= { lairdSnmpCliStatus 8 }    
+    
+END
