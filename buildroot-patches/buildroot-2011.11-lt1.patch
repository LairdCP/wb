+LAIRD-MIB DEFINITIONS ::= BEGIN
+
+--
+-- Top-level infrastructure of the Laired enterprise MIB tree
+-- Based on Net-SNMP MIB 
+--
+
+IMPORTS
+    MODULE-IDENTITY, enterprises FROM SNMPv2-SMI;
+
+laird MODULE-IDENTITY
+    LAST-UPDATED "201208160000Z"
+    ORGANIZATION "www.lairdtech.com"
+    CONTACT-INFO    
+	 "postal:   Tim Carney
+                    11160 Thompson Ave
+                    Lenexa, KS 66219 USA
+
+          email:    wireless-support@lairdtech.com"
+    DESCRIPTION
+	"Top-level infrastructure of the Laird enterprise MIB tree"
+    REVISION     "201208160000Z"
+    DESCRIPTION
+	"First draft"
+    ::= { enterprises 40412}
+--40412 is a placeholder until we receive an official enterprise number from IANA
+
+--
+--  Laird enterprise-specific management objects
+--
+
+lairdObjects              OBJECT IDENTIFIER ::= {laird 1}
+lairdExamples             OBJECT IDENTIFIER ::= {laird 2}
+--lairdModuleIDs            OBJECT IDENTIFIER ::= {lairdEnumerations 1}
+--lairdAgentOIDs            OBJECT IDENTIFIER ::= {lairdEnumerations 2}
+lairdSnmpExperimental     OBJECT IDENTIFIER ::= {laird 9999}
+
+--
+-- A subtree specifically designed for private testing and development purposes.
+-- No "public" management objects should ever be defined within this tree.
+--
+-- It is provided for private experimentation, prior to transferring a MIB
+-- structure to another part of the overall OID tree
+--
+lairdPlaypen              OBJECT IDENTIFIER ::= {laird 9999}
+
+
+--
+--  Notifications
+--
+
+lairdNotificationPrefix   OBJECT IDENTIFIER ::= {laird 4}
+lairdNotifications        OBJECT IDENTIFIER ::= {lairdNotificationPrefix 0}
+lairdNotificationObjects  OBJECT IDENTIFIER ::= {lairdNotificationPrefix 1}
+
+
+--
+--  Conformance
+--
+
+lairdConformance          OBJECT IDENTIFIER ::= {laird 5}
+lairdCompliances          OBJECT IDENTIFIER ::= {lairdConformance 1}
+lairdGroups               OBJECT IDENTIFIER ::= {lairdConformance 2}
+
+END
+
diff -Naur old/package/netsnmp/netsnmp-laird_mib.patch new/package/netsnmp/netsnmp-laird_mib.patch
--- old/package/netsnmp/netsnmp-laird_mib.patch	1970-01-01 01:00:00.000000000 +0100
+++ new/package/netsnmp/netsnmp-laird_mib.patch	2012-10-04 17:49:01.984375000 +0100
@@ -0,0 +1,329 @@
+--- netsnmp-5.7.1.orig/agent/mibgroup/laird-mib/laird_scalars.c	1969-12-31 19:00:00.000000000 -0500
++++ netsnmp-5.7.1/agent/mibgroup/laird-mib/laird_scalars.c	2012-09-11 14:13:38.749815996 -0400
+@@ -0,0 +1,114 @@
++/**  @LAIRD laird_scalars.c
++ *  This creates scalar registrations for all of the Laird-specific
++ *  scalar variables to be accessed via SNMP.  
++ *
++ *  If this module is compiled into an agent, you should be able to
++ *  issue snmp commands that look something like (authentication
++ *  information not shown in these commands):
++ *
++ *
++ */
++
++/*
++ * start be including the appropriate header files 
++ */
++#include <net-snmp/net-snmp-config.h>
++#include <net-snmp/net-snmp-features.h>
++#include <net-snmp/net-snmp-includes.h>
++#include <net-snmp/agent/net-snmp-agent-includes.h>
++#include <laird-mib.h>
++
++/*
++ * if --enable-minimalist has been turned on, we need to register
++ * the support we need so the needed functions aren't removed at compile time
++ */
++netsnmp_feature_require(long_instance)
++
++/*
++ * Then, we declare the variables we want to be accessed 
++ */
++static long     statusChannel = 12;       /* default value */
++static long     statusRSSI = -57;          /* default value */
++static long     statusKBitRate = 2000;      /* default value */
++static long     statusTxPower = 5;       /* default value */
++static long     statusBeaconPeriod = 121;  /* default value */
++
++/*
++ * our initialization routine, automatically called by the agent 
++ * (to get called, the function name must match init_FILENAME())
++ */
++void init_laird_scalars(void)
++{
++    /*
++     * the OID we want to register our integer at.  This should be a
++     * fully qualified instance.  In our case, it's a scalar at:
++     * NET-SNMP-EXAMPLES-MIB::netSnmpExampleInteger.0 (note the
++     * trailing 0 which is required for any instantiation of any
++     * scalar object) 
++     */
++    oid statusChannel_oid[] =       { 1, 3, 6, 1, 4, 1, LAIRD_PEN, 3,  2, 0 };
++    oid statusRSSI_oid[] =          { 1, 3, 6, 1, 4, 1, LAIRD_PEN, 3,  3, 0 };
++    oid statusKBitRate_oid[] =      { 1, 3, 6, 1, 4, 1, LAIRD_PEN, 3, 10, 0 };
++    oid statusTxPower_oid[] =       { 1, 3, 6, 1, 4, 1, LAIRD_PEN, 3, 11, 0 };
++    oid statusBeaconPeriod_oid[] =  { 1, 3, 6, 1, 4, 1, LAIRD_PEN, 3, 12, 0 };
++    
++    /*
++     * the line below registers our "example1" variable above as
++     * accessible and makes it writable.  A read only version of the
++     * same registration would merely call
++     * register_read_only_long_instance() instead.
++     * 
++     * If we wanted a callback when the value was retrieved or set
++     * (even though the details of doing this are handled for you),
++     * you could change the NULL pointer below to a valid handler
++     * function. 
++     */
++/*    
++    netsnmp_register_long_instance("my example int variable",
++                                  my_registration_oid,
++                                  OID_LENGTH(my_registration_oid),
++                                  &example1, NULL);
++
++    DEBUGMSGTL(("example_scalar_int",
++    "Done initalizing example scalar int\n"));
++*/
++    /*
++    netsnmp_register_scalar(netsnmp_create_handler_registration
++                             ("ipForwarding", handle_ipForwarding,
++                              ipForwarding_oid,
++                              OID_LENGTH(ipForwarding_oid),
++                              HANDLER_CAN_RWRITE));
++    */
++
++/* Integer32 variables */    
++    netsnmp_register_long_instance("Channel variable",
++                                    statusChannel_oid,
++                                    OID_LENGTH(statusChannel_oid),
++                                    &statusChannel,
++                                    NULL);
++                                    
++    netsnmp_register_long_instance("RSSI variable",
++                                    statusRSSI_oid,
++                                    OID_LENGTH(statusRSSI_oid),
++                                    &statusRSSI,
++                                    NULL);
++
++    netsnmp_register_long_instance("KBitRate string variable",
++                                    statusKBitRate_oid,
++                                    OID_LENGTH(statusKBitRate_oid),
++                                    &statusKBitRate,
++                                    NULL);
++
++    netsnmp_register_long_instance("TxPower variable",
++                                    statusTxPower_oid,
++                                    OID_LENGTH(statusTxPower_oid),
++                                    &statusTxPower,
++                                    NULL);
++
++    netsnmp_register_long_instance("Beacon Period variable",
++                                    statusBeaconPeriod_oid,
++                                    OID_LENGTH(statusBeaconPeriod_oid),
++                                    &statusBeaconPeriod,
++                                    NULL);                                    
++
++}
+--- netsnmp-5.6.1.1.orig/agent/mibgroup/laird-mib/laird_scalars.h	1969-12-31 19:00:00.000000000 -0500
++++ netsnmp-5.6.1.1/agent/mibgroup/laird-mib/laird_scalars.h	2012-09-11 14:13:38.545816001 -0400
+@@ -0,0 +1,14 @@
