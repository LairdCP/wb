++
++    netsnmp_register_long_instance("Beacon Period variable",
++                                    statusBeaconPeriod_oid,
++                                    OID_LENGTH(statusBeaconPeriod_oid),
++                                    &statusBeaconPeriod,
++                                    NULL);                                    
++
++}
